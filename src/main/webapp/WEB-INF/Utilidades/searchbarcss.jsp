.topnav {
    border-radius:3px;
    overflow: hidden;
    background-color: #299be4;
}

.topnav a {
    float: left;
    display: block;
    color: #fff;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
    font-size: 17px;
}

.topnav a:hover {
    background-color: #f5f5f5;
    color: black;
    
}

.topnav a.active {
    background-color: #2196F3;
    color: white;
}
